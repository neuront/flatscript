ls: [5, 1, 6, 7, 2, 9, 4, 3, 8, 0]

console.log(ls |: $ * $)
console.log(ls |: $ % 2 = 0)
console.log(ls |? $ % 2 = 0)

func head(list, n)
    return list |? $i < n

func after(list, n)
    return list |? $i >= n

console.log(head(ls, 4))
console.log(after(ls, 4))
console.log(ls |: $ * $ % 7 |? $ < 4)

